#include <stdio.h>

int main() {
    int marks[5], i, total = 0;
    float average;
    char grade;

    printf("Enter marks of 5 subjects: ");
    for(i = 0; i < 5; i++) {
        scanf("%d", &marks[i]);
        total += marks[i];
    }

    average = total / 5.0;

    if(average >= 90)
        grade = 'A';
    else if(average >= 75)
        grade = 'B';
    else if(average >= 60)
        grade = 'C';
    else if(average >= 50)
        grade = 'D';
    else
        grade = 'F';

    printf("\nTotal = %d", total);
    printf("\nAverage = %.2f", average);
    printf("\nGrade = %c\n", grade);

    return 0;
}
