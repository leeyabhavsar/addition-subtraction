#include <stdio.h>

int multiply(int *arr, int n) {
    int i, prod = 1;
    for(i = 0; i < n; i++)
        prod *= *(arr + i);
    return prod;
}

int main() {
    int arr[5], i, result;

    printf("Enter 5 numbers: ");
    for(i = 0; i < 5; i++)
        scanf("%d", &arr[i]);

    result = multiply(arr, 5);
    printf("Product of all elements = %d\n", result);

    return 0;
}
