#include <stdio.h>

void swapValue(int a, int b) {
    int temp = a;
    a = b;
    b = temp;
    printf("After swap (call by value): a = %d, b = %d\n", a, b);
}

void swapRef(int *a, int *b) {
    int temp = *a;
    *a = *b;
    *b = temp;
}

int main() {
    int x, y;
    printf("Enter two numbers: ");
    scanf("%d %d", &x, &y);

    swapValue(x, y);
    swapRef(&x, &y);
    printf("After swap (call by reference): x = %d, y = %d\n", x, y);

    return 0;
}
